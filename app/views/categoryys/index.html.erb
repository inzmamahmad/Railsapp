<p id="notice"><%= notice %></p>

<h1>Categorys</h1>





<table class="table">
  <thead>
    <tr>
      <th scope="col">Category Name</th>
      <th scope="col">User ID</th>
      <th scope="col">Action</th>
    </tr>
  </thead>
  <tbody>
     <% @categoryys.each do |categoryy| %>
      <tr>
        <td><%= categoryy.name %></td>
        <td><%= categoryy.user_id %></td>
        <td><%= link_to 'Show', categoryy %></td>
        <td><%= link_to 'Edit', edit_categoryy_path(categoryy) %></td>
        <td><%= link_to 'Destroy', categoryy, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
      <%end%>
  </tbody>
</table>
<button type="button" class="btn btn-secondary"><%= link_to 'New Categoryy', new_categoryy_path %></button>


